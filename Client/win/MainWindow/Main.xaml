        						</MultiTrigger.Conditions>
        						<Setter Property="Background" TargetName="border">
        							<Setter.Value>
        								<ImageBrush ImageSource="/TrboX;component/resource/images/full_screen_14_14_h.png"/>
        							</Setter.Value>
