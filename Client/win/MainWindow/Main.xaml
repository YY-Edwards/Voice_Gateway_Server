                <DataTemplate>
                    <DockPanel Width="245" Height="336" >
                        <!--<DockPanel.ContextMenu >
                            <ContextMenu Visibility="Hidden"/>
                        </DockPanel.ContextMenu>-->
                        <local:FastPanel x:Name="fp_ImageIcon"  PanelItem ="{Binding}" AllowDrop="True" TitlePressed="FastPanel_TiltePressed" Closing = "FastPanel_Closing" IsRepeater="{Binding Visibility, Converter={StaticResource IsHide}, ElementName=btn_Tool_Check}"
