    					<Border x:Name="border" BorderBrush="Black" BorderThickness="0" Background="{x:Null}">
    						<Grid Height="Auto" Margin="0">
    							<ed:RegularPolygon x:Name="regularPolygon" Fill="#FFB2B6B9" Height="5" InnerRadius="1" Margin="-3,0,-1,0" PointCount="3" Stretch="Fill" Stroke="Black" Width="12" RenderTransformOrigin="0.5,0.5" StrokeThickness="0" VerticalAlignment="Center" HorizontalAlignment="Left">
    								<ed:RegularPolygon.RenderTransform>
    									<TransformGroup>
    										<ScaleTransform/>
    										<SkewTransform/>
    										<RotateTransform Angle="-90"/>
    										<TranslateTransform/>
    									</TransformGroup>
    								</ed:RegularPolygon.RenderTransform>
    							</ed:RegularPolygon>
    							<ed:RegularPolygon x:Name="regularPolygon1" Fill="#FFB2B6B9" Height="5" InnerRadius="1" Margin="-1,0,-3,0" PointCount="3" Stretch="Fill" Stroke="Black" Width="12" RenderTransformOrigin="0.5,0.5" StrokeThickness="0" VerticalAlignment="Center" HorizontalAlignment="Center">
    								<ed:RegularPolygon.RenderTransform>
    									<TransformGroup>
    										<ScaleTransform/>
    										<SkewTransform/>
    										<RotateTransform Angle="90"/>
    										<TranslateTransform/>
    									</TransformGroup>
    								</ed:RegularPolygon.RenderTransform>
    							</ed:RegularPolygon>
    						</Grid>
    					</Border>
    					<ControlTemplate.Triggers>
    						<Trigger Property="IsMouseOver" Value="True">
    							<Setter Property="Fill" TargetName="regularPolygon" Value="#FF5695DB"/>
    							<Setter Property="Fill" TargetName="regularPolygon1" Value="#FF5695DB"/>
    						</Trigger>
    						<Trigger Property="IsChecked" Value="True">
    							<Setter Property="Visibility" TargetName="regularPolygon1" Value="Hidden"/>
    						</Trigger>
    						<Trigger Property="IsChecked" Value="False">
    							<Setter Property="Visibility" TargetName="regularPolygon" Value="Hidden"/>
    						</Trigger>
    						<Trigger Property="HasContent" Value="true">
    							<Setter Property="FocusVisualStyle" Value="{StaticResource CheckRadioFocusVisual}"/>
    							<Setter Property="Padding" Value="4,0,0,0"/>
    						</Trigger>
    						<Trigger Property="IsEnabled" Value="false">
    							<Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.GrayTextBrushKey}}"/>
    						</Trigger>
    					</ControlTemplate.Triggers>
    				</ControlTemplate>
    			</Setter.Value>
    		</Setter>
    	</Style>

