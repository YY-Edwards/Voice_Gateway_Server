<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:Controls="clr-namespace:Sigmar.Controls;assembly=Sigmar" 
             xmlns:ctrl="clr-namespace:Dispatcher.Controls"
             xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity" 
             xmlns:Service="clr-namespace:Dispatcher.Service" xmlns:ViewsModules="clr-namespace:Dispatcher.ViewsModules" x:Name="UserControl" x:Class="Dispatcher.Views.NewQuickOperation" 
             mc:Ignorable="d" 
              d:DesignHeight="300" d:DesignWidth="600">
    <UserControl.Resources>
        <Service:EnableTargetMultipleConverter x:Key="EnableTargetMultipleConverter"/>
        <Service:LocationCycleVisibleConverter x:Key="LocationCycleVisibleConverter"/>
    </UserControl.Resources>
    <UserControl.DataContext>
        <ViewsModules:VMQuickList/>
    </UserControl.DataContext>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="220"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="220"/>
        </Grid.ColumnDefinitions>
        <TextBlock TextWrapping="Wrap" Text="新建操作" HorizontalAlignment="Center" Foreground="#FFA8CBF0" FontSize="14" FontFamily="Hiragino Sans GB W3" VerticalAlignment="Center"/>
        <TextBlock TextWrapping="Wrap" Text="快速操作列表" DockPanel.Dock="Top" HorizontalAlignment="Center" Foreground="#FFA8CBF0" FontSize="14" FontFamily="Hiragino Sans GB W3" Grid.Column="2" VerticalAlignment="Center"/>

        <Grid Background="#FF333335" Grid.Row="1" Margin="0,0,0,0" >
            <Grid x:Name="grid"  Margin="12,5,12,24" DataContext="{Binding Operation}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="40"/>
                    <RowDefinition Height="45"/>
                    <RowDefinition Height="86*"/>
                </Grid.RowDefinitions>

                <ComboBox x:Name="cmb_OpType" Margin="0" VerticalAlignment="Center" SelectedIndex="{Binding DataContext.OperateTypeIndex,ElementName=UserControl}" DockPanel.Dock="Top" Style="{DynamicResource ComboBoxStyleNormal}" Height="30" Foreground="#FFB6BABE" >
                    <ComboBoxItem Content="呼叫" Style="{DynamicResource ComboBoxItemStyleNormal}" Visibility="{x:Static Service:FunctionConfigure.ScheduleVisible}" Height="32" Foreground="#FFD2DFF5" FontSize="13"/>
                    <ComboBoxItem Content="短信" Style="{DynamicResource ComboBoxItemStyleNormal}"  Visibility="{x:Static Service:FunctionConfigure.ShortMessageVisible}" Height="32" Foreground="#FFD2DFF5" FontSize="13"/>
                    <ComboBoxItem Content="信令控制" Style="{DynamicResource ComboBoxItemStyleNormal}"  Visibility="{x:Static Service:FunctionConfigure.ControlerVisible}" Height="32" Foreground="#FFD2DFF5" FontSize="13"/>
                    <ComboBoxItem Content="位置查询" Style="{DynamicResource ComboBoxItemStyleNormal}"  Visibility="{x:Static Service:FunctionConfigure.LocationVisible}" Height="32" Foreground="#FFD2DFF5" FontSize="13"/>
                    <ComboBoxItem Content="室内定位" Style="{DynamicResource ComboBoxItemStyleNormal}"  Visibility="{x:Static Service:FunctionConfigure.LocationInDoorVisible}" Height="32" Foreground="#FFD2DFF5" FontSize="13"/>
                    <ComboBoxItem Content="工单" Style="{DynamicResource ComboBoxItemStyleNormal}"  Visibility="{x:Static Service:FunctionConfigure.JobTicketVisible}" Height="32" Foreground="#FFD2DFF5" FontSize="13" />
                </ComboBox>
                <ctrl:TargetSelector x:Name="targetSelector" Height="30" SelectedItems="{Binding Targets, Mode=TwoWay, NotifyOnTargetUpdated=True, NotifyOnSourceUpdated=True, ValidatesOnNotifyDataErrors=False}" Grid.Row="1"  BorderBrush="#FF505053" BorderThickness="1" ItemSource="{Binding DataContext.AllTarget, ElementName=UserControl}" VerticalAlignment="Center"  PopupHorizontalOffset="-1" PopupVerticalOffset="2" IsMultiple="{Binding SelectedIndex, Converter={StaticResource EnableTargetMultipleConverter}, ElementName=cmb_OpType}"  />
                <TabControl BorderThickness="0" Background="{x:Null}" Grid.Row="2" Style="{DynamicResource WorkTabControlStyle}" SelectedIndex="{Binding SelectedIndex, ElementName=cmb_OpType}">
                    <TabItem Header="呼叫"/>
                    <TabItem Header="短信">
                        <StackPanel>
                            <Label Content="内容："  Grid.Row="2"  Foreground="#FFB6BABE" FontSize="13" HorizontalAlignment="Left" VerticalAlignment="Top" Height="35" Padding="0" VerticalContentAlignment="Center" Margin="5,0,0,0"/>
                            <TextBox Height="80" TextWrapping="Wrap" Text="{Binding ShortMessageContents}" Style="{DynamicResource TextBoxStyleNormal}" Foreground="#FFB6BABE" CaretBrush="#FFB6BABE"/>
                        </StackPanel>
                    </TabItem>
                    <TabItem Header="信令控制">
                        <StackPanel>
                            <Label Content="指令："  Foreground="#FFB6BABE" FontSize="13" HorizontalAlignment="Left" VerticalAlignment="Top" Height="35" Padding="0" VerticalContentAlignment="Center" Margin="5,0,0,0"/>
                            <ComboBox VerticalAlignment="Center" SelectedIndex="{Binding ControlTypeIndex}"  Foreground="#FFB6BABE" FontSize="13" Style="{DynamicResource ComboBoxStyleNormal}" Grid.Row="2" Grid.Column="1" Height="30">
                                <ComboBoxItem Content="在线检测" Foreground="#FFD2DFF5" Height="32" Style="{DynamicResource ComboBoxItemStyleNormal}" FontSize="13"/>
                                <ComboBoxItem Content="远程监听"  Foreground="#FFD2DFF5" Height="32" Style="{DynamicResource ComboBoxItemStyleNormal}" FontSize="13"/>
                                <ComboBoxItem Content="遥毙" Foreground="#FFD2DFF5" Height="32" Style="{DynamicResource ComboBoxItemStyleNormal}" FontSize="13"/>
                                <ComboBoxItem Content="遥开" Foreground="#FFD2DFF5" Height="32" Style="{DynamicResource ComboBoxItemStyleNormal}" FontSize="13"/>
                            </ComboBox>
                        </StackPanel>
                    </TabItem>

                    <TabItem Header="位置查询">
                        <StackPanel>
                            <Label Content="查询方式："  Foreground="#FFB6BABE" FontSize="13" HorizontalAlignment="Left" VerticalAlignment="Top" Height="35" Padding="0" VerticalContentAlignment="Center" Margin="5,0,0,0"/>
                            <ComboBox x:Name="comboBox" VerticalAlignment="Center"  SelectedIndex="{Binding LocationTypeIndex}"   Foreground="#FFB6BABE" FontSize="13" Style="{DynamicResource ComboBoxStyleNormal}" Grid.Row="2" Grid.Column="1" Height="30">
                                <ComboBoxItem Content="查询" Tag="Query" Foreground="#FFD2DFF5" Height="32" Style="{DynamicResource ComboBoxItemStyleNormal}" FontSize="13"/>
                                <ComboBoxItem Content="周期查询" Tag="Cycle" Foreground="#FFD2DFF5" Height="32" Style="{DynamicResource ComboBoxItemStyleNormal}" FontSize="13"/>
                                <ComboBoxItem Content="CSBK查询" Tag="CsbkQuery" Foreground="#FFD2DFF5" Height="32" Style="{DynamicResource ComboBoxItemStyleNormal}" FontSize="13"/>
                                <ComboBoxItem Content="CSBK周期查询" Tag="CsbkCycle" Foreground="#FFD2DFF5" Height="32" Style="{DynamicResource ComboBoxItemStyleNormal}" FontSize="13"/>
                                <ComboBoxItem Content="增强型CSBK查询" Tag="EnhCsbkQuery" Foreground="#FFD2DFF5" Height="32" Style="{DynamicResource ComboBoxItemStyleNormal}" FontSize="13"/>
                                <ComboBoxItem Content="增强型CSBK周期查询" Tag="EnhCsbkCycle" Foreground="#FFD2DFF5" Height="32" Style="{DynamicResource ComboBoxItemStyleNormal}" FontSize="13"/>
                                <ComboBoxItem Content="停止周期查询" Tag="StopCycle" Foreground="#FFD2DFF5" Height="32" Style="{DynamicResource ComboBoxItemStyleNormal}" FontSize="13"/>
                                <ComboBoxItem Content="轨迹查询" Tag="Track" Foreground="#FFD2DFF5" Height="32" Style="{DynamicResource ComboBoxItemStyleNormal}" FontSize="13"/>
                            </ComboBox>
                            <Label Content="查询周期："  Grid.Row="2"  Foreground="#FFB6BABE" FontSize="13" HorizontalAlignment="Left" VerticalAlignment="Top" Height="35" Padding="0" VerticalContentAlignment="Center" Margin="5,0,0,0" Visibility="{Binding SelectedIndex, Converter={StaticResource LocationCycleVisibleConverter}, ElementName=comboBox}"/>
                            <TextBox Height="30" TextWrapping="Wrap" Text="{Binding LocationCycle}" Style="{DynamicResource TextBoxStyleNormal}" Foreground="#FFB6BABE" CaretBrush="#FFB6BABE" Visibility="{Binding SelectedIndex, Converter={StaticResource LocationCycleVisibleConverter}, ElementName=comboBox}"/>
                        </StackPanel>
                    </TabItem>
                    <TabItem Header="室内定位">
                        <StackPanel>
                            <Label Content="启动/停止："  Foreground="#FFB6BABE" FontSize="13" HorizontalAlignment="Left" VerticalAlignment="Top" Height="35" Padding="0" VerticalContentAlignment="Center" Margin="5,0,0,0"/>
                            <ComboBox VerticalAlignment="Center"  SelectedIndex="{Binding LocationInDoorTypeIndex}"   Foreground="#FFB6BABE" FontSize="13" Style="{DynamicResource ComboBoxStyleNormal}" Grid.Row="2" Grid.Column="1" Height="30">
                                <ComboBoxItem Content="启动" Foreground="#FFD2DFF5" Height="32" Style="{DynamicResource ComboBoxItemStyleNormal}" FontSize="13"/>
                                <ComboBoxItem Content="停止" Foreground="#FFD2DFF5" Height="32" Style="{DynamicResource ComboBoxItemStyleNormal}" FontSize="13"/>
                            </ComboBox>
                        </StackPanel>
                    </TabItem>
                    <TabItem Header="工单"/>
                </TabControl>
            </Grid>
        </Grid>

        <ListView x:Name="listView1"  ItemsSource="{Binding QuickOperationList, Mode=OneWay}" BorderThickness="0"  ScrollViewer.VerticalScrollBarVisibility="Auto" Background="#FF333335" Margin="0"  HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" ScrollViewer.HorizontalScrollBarVisibility="Hidden" Width="218" Grid.Column="2" Grid.Row="1" Style="{DynamicResource ListViewStyleNormal}" >
            <ListView.View>
                <ctrl:ResourceView>
                    <ctrl:ResourceView.ItemTemplate>
                        <DataTemplate>
                            <Border x:Name="Msg" HorizontalAlignment="Stretch"  VerticalAlignment="Stretch" Padding="0" Height="32">
                                <DockPanel  Opacity="{Binding ViewModule.OnlineOpacity}">
                                    <Grid HorizontalAlignment="Left" Height="20"  Width="20" DockPanel.Dock="Left" Margin="14,0,0,0" >
                                        <Ellipse Margin="0" Stroke="Black" StrokeThickness="0" Fill="{Binding IconBackGround}" />
                                        <Image  Source="{Binding ViewModule.Icon}"  Width="12" Height="12" VerticalAlignment="Center" HorizontalAlignment="Center" />
                                    </Grid>
                                    <TextBlock Text="{Binding ViewModule.FullName}"  TextWrapping="NoWrap" Margin="5,0,17,0" VerticalAlignment="Center" DockPanel.Dock="Left" Height="Auto" Foreground="#FFD2DFF5" FontSize="13" FontFamily="Hiragino Sans GB W3"/>
                                </DockPanel>
                            </Border>
                        </DataTemplate>
                    </ctrl:ResourceView.ItemTemplate>
                </ctrl:ResourceView>
            </ListView.View>
        </ListView>
        <Grid Grid.ColumnSpan="3" Grid.RowSpan="2">
            <Grid.RowDefinitions>
                <RowDefinition Height="50*"/>
                <RowDefinition Height="50*"/>
            </Grid.RowDefinitions>
            <Button Content="=&gt;" Command="{Binding Add, Mode=OneWay}"  VerticalAlignment="Bottom" Style="{DynamicResource ButtonStyleFastSend}" Width="90" Margin="0,9.5" CommandParameter="{Binding DataContext, ElementName=grid}"/>
            <Button Content="&lt;="  Command="{Binding Remove, Mode=OneWay}" Grid.RowSpan="2" Grid.Row="1" VerticalAlignment="Top"  Style="{DynamicResource ButtonStyleUnSend}" Width="90" Height="32" Margin="0,9.5" CommandParameter="{Binding SelectedItem, ElementName=listView1}" />
        </Grid>

