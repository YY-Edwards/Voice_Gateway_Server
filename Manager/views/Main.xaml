<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
        xmlns:local="clr-namespace:Manager"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        xmlns:WPF="clr-namespace:Controls.WPF;assembly=Controls.WPF" x:Name="win_Main" 
        mc:Ignorable="d" 
        x:Class="Manager.Main"
        Title="MainWindow" Height="728" Width="920" AllowsTransparency="True" WindowStyle="None" Background="#FF1C1D1E">
    <Window.Resources>
        <local:CVMPayout x:Key="CVMPayout"/>
        <local:CVMConfiguration x:Key="CVMConfiguration" />
        <local:MultiParameter x:Key="MultiParameter" />

        <local:ServerStatusConverter x:Key="ServerStatusConverter" />
        <local:ConnectBtnConverter x:Key="ConnectBtnConverter" />
        <local:TrueVisable x:Key="TrueVisable" />
        <local:FalseHide x:Key="FalseHide" />

        <ResourceDictionary x:Key="CustomStyle">
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="style.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <i:Interaction.Triggers>
        <i:EventTrigger EventName="Loaded">
            <i:InvokeCommandAction Command="{Binding Load, Mode=OneWay, Source={StaticResource CVMPayout}}" CommandParameter="{Binding ElementName=win_Main, Mode=OneWay}" />
            <i:InvokeCommandAction Command="{Binding Load, Mode=OneWay, Source={StaticResource CVMConfiguration}}" CommandParameter="{Binding ElementName=win_Main, Mode=OneWay}" />
        </i:EventTrigger>
        <i:EventTrigger EventName="Closed">
            <i:InvokeCommandAction Command="{Binding Exit, Mode=OneWay, Source={StaticResource CVMPayout}}"/>
        </i:EventTrigger>
    </i:Interaction.Triggers>
    <Grid>
        <DockPanel>
            <Grid x:Name="Header" Height="40" VerticalAlignment="Top" DockPanel.Dock="Top">
                <Border BorderBrush="Black" BorderThickness="0" Background="#FF3B3B3D">
                    <i:Interaction.Triggers>
                        <i:EventTrigger EventName="PreviewMouseLeftButtonDown">
                            <i:InvokeCommandAction Command="{Binding Move, Mode=OneWay, Source={StaticResource CVMPayout}}" CommandParameter="{Binding ElementName=win_Main, Mode=OneWay}" />
                        </i:EventTrigger>
                    </i:Interaction.Triggers>
                    <DockPanel LastChildFill="False" Margin="0,0,0,-2" HorizontalAlignment="Left" Width="322">
                        <Image Margin="22,0,0,0" Source="images/sub_title_60_20.png" Height="20" Width="60"/>
                        <Label Content="3.0" VerticalAlignment="Top" Foreground="#FF7DABDC" Margin="5,6,0,0" VerticalContentAlignment="Center" FontSize="15" Padding="5,6,5,5" FontFamily="Hiragino Sans GB W3"/>
                        <Label Content="management" VerticalAlignment="Top" Foreground="#FF7DABDC" FontSize="15" Padding="5,5.5,5,5" Margin="0,6,0,0" FontFamily="Hiragino Sans GB W3"/>
                    </DockPanel>
                </Border>
                <DockPanel HorizontalAlignment="Right" LastChildFill="False">
                    <WPF:IconBtn Height="18" VerticalAlignment="Center" Width="18" Margin="8,0,11,0" DockPanel.Dock="Right" Normal="images/close_18_18_n.png" Hover="images/close_18_18_h.png" Press="images/close_18_18_h.png">
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="Click">
                                <i:InvokeCommandAction Command="{Binding Close, Mode=OneWay, Source={StaticResource CVMPayout}}" CommandParameter="{Binding ElementName=win_Main, Mode=OneWay}" />
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </WPF:IconBtn>
                    <WPF:IconBtn Height="18" VerticalAlignment="Center" Width="18" Margin="8,11" DockPanel.Dock="Right" Normal="images/min_18_18_n.png" Hover="images/min_18_18_h.png" Press="images/min_18_18_h.png">
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="Click">
                                <i:InvokeCommandAction Command="{Binding Minimize, Mode=OneWay, Source={StaticResource CVMPayout}}" CommandParameter="{Binding ElementName=win_Main, Mode=OneWay}" />
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                    </WPF:IconBtn>
                </DockPanel>
                <Path Data="M920,2L0,2 0,0 920,0z" VerticalAlignment="Bottom">
                    <Path.Fill>
                        <LinearGradientBrush EndPoint="0.5,0" StartPoint="0.5,1">
                            <GradientStop Color="#FF636161" Offset="0"/>
                            <GradientStop Color="#FF4B4847" Offset="0.16"/>
                            <GradientStop Color="#FF231915" Offset="0.243"/>
                            <GradientStop Color="#FF848484" Offset="1"/>
                        </LinearGradientBrush>
                    </Path.Fill>
                    <Path.OpacityMask>
                        <LinearGradientBrush EndPoint="0.5,0" StartPoint="0.5,1">
                            <GradientStop Color="#00000000" Offset="0"/>
                            <GradientStop Color="#59000000" Offset="0.16"/>
                            <GradientStop Color="#B2000000" Offset="0.243"/>
                            <GradientStop Color="#00000000" Offset="1"/>
                        </LinearGradientBrush>
                    </Path.OpacityMask>
                </Path>
            </Grid>
            <Border x:Name="Status_Operate" BorderBrush="Black" Height="60" VerticalAlignment="Top" DockPanel.Dock="Bottom">
                <DockPanel LastChildFill="False" Margin="16,0">

                    <Image HorizontalAlignment="Left" Height="15" VerticalAlignment="Center" Width="19" Source="images/notice_19_15_n.png" Margin="0,0,10,0"/>

                    <TextBlock TextWrapping="Wrap"  VerticalAlignment="Center" Foreground="#FFCFCFCF" FontSize="14" FontFamily="Hiragino Sans GB W3">
                        <TextBlock.Text>
                            <MultiBinding Converter="{StaticResource ServerStatusConverter}">
                                <Binding Source="{StaticResource CVMConfiguration}" Path="IsConnect"/>
                                <Binding Source="{StaticResource CVMConfiguration}" Path="IsConnect"/>
                            </MultiBinding>
                        </TextBlock.Text>
                    </TextBlock>
                    <Button Content="连接服务" Height="30" VerticalAlignment="Center" Width="90" HorizontalAlignment="Right" Style="{DynamicResource ButtonNormalStyle}" Tag="4" Foreground="#FFCFCFCF" FontSize="14" FontFamily="Hiragino Sans GB W3" BorderBrush="#FF5C5C5D" Background="#FF3B3B3D" Command="{Binding ConnectServer, Mode=OneWay, Source={StaticResource CVMPayout}}" Margin="10,0,0,0">
                        <Button.CommandParameter>
                            <MultiBinding Converter="{StaticResource MultiParameter}">
                                <Binding Source="{StaticResource CVMConfiguration}"/>
                                <Binding Source="{StaticResource CVMPayout}"/>
                            </MultiBinding>
                        </Button.CommandParameter>
                        <Button.Visibility>
                            <MultiBinding Converter="{StaticResource ConnectBtnConverter}">
                                <Binding Source="{StaticResource CVMConfiguration}" Path="IsConnect"/>
                                <Binding Source="{StaticResource CVMConfiguration}" Path="IsConnect"/>
                            </MultiBinding>
                        </Button.Visibility>
                    </Button>
                    <Button Content="取消" Height="30" VerticalAlignment="Center" Width="90" DockPanel.Dock="Right" Margin="0,15,0,13" HorizontalAlignment="Right" Style="{DynamicResource ButtonNormalStyle}" Tag="4" Foreground="#FFCFCFCF" FontSize="14" FontFamily="Hiragino Sans GB W3" BorderBrush="#FF5C5C5D" Background="#FF3B3B3D" Command="{Binding Close, Mode=OneWay, Source={StaticResource CVMPayout}}" CommandParameter="{Binding ElementName=win_Main, Mode=OneWay}"/>
                    <Button Content="保存" Height="30" VerticalAlignment="Center" Width="90" DockPanel.Dock="Right" Margin="0,15,10,13" HorizontalAlignment="Right" Style="{DynamicResource ButtonNormalStyle}" Tag="4"  Foreground="#FFCFCFCF" FontSize="14" FontFamily="Hiragino Sans GB W3" BorderBrush="#FF5C5C5D" Background="#FF3B3B3D" Command="{Binding Save, Mode=OneWay, Source={StaticResource CVMPayout}}">
                        <Button.CommandParameter>
                            <MultiBinding Converter="{StaticResource MultiParameter}">
                                <Binding ElementName="win_Main"/>
                                <Binding Source="{StaticResource CVMConfiguration}"/>
                                <Binding Source="{StaticResource CVMPayout}"/>
                            </MultiBinding>
                        </Button.CommandParameter>
                    </Button>
                </DockPanel>
            </Border>
            <Border x:Name="MainBoard" BorderBrush="Black" DockPanel.Dock="Top">
                <DockPanel Margin="16,12,16,0">
                    <Border BorderBrush="#FF0C0C0D" BorderThickness="1" Background="#FF313133" Margin="0,0,10,0" Width="197">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="31"/>
                                <RowDefinition Height="73*"/>
                            </Grid.RowDefinitions>
                            <Label Content="配置项" Background="#FF3B3B3D" VerticalContentAlignment="Center" BorderThickness="0,0,0,1" Height="31" Foreground="#FFE4E4E4" BorderBrush="#FF0C0C0D" FontSize="14" FontFamily="Hiragino Sans GB W3" Padding="10,5,5,5"/>
                            <TreeView x:Name="tree_Options" Margin="0,10" Grid.Row="1" BorderThickness="0" Background="{x:Null}" ScrollViewer.HorizontalScrollBarVisibility="Disabled" Padding="0" Style="{DynamicResource TreeViewStyleNav}">
                                <TreeViewItem Header="基本设置" Style="{DynamicResource TreeViewItemStyleRoot}" IsSelected="True" IsExpanded="True" >
                                    <i:Interaction.Triggers>
                                        <i:EventTrigger EventName="Selected">
                                            <i:InvokeCommandAction Command="{Binding TreeSelectedChanged, Mode=OneWay, Source={StaticResource CVMPayout}}" CommandParameter="0" />
                                        </i:EventTrigger>
                                    </i:Interaction.Triggers>
                                    <TreeViewItem Header="常规" Style="{DynamicResource TreeViewItemStyle2nd}" Visibility="Collapsed"/>
                                    <TreeViewItem  Header="网络" Style="{DynamicResource TreeViewItemStyle2nd}" >
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="PreviewMouseLeftButtonUp">
                                                <i:InvokeCommandAction Command="{Binding ScrollToItem, Mode=OneWay, Source={StaticResource CVMPayout}}" >
                                                    <i:InvokeCommandAction.CommandParameter>
                                                        <MultiBinding Converter="{StaticResource MultiParameter}">
                                                            <Binding ElementName="bdr_Options"/>
                                                            <Binding ElementName="scr_Base"/>
                                                            <Binding ElementName="dck_Base_Net"/>
                                                        </MultiBinding>
                                                    </i:InvokeCommandAction.CommandParameter>
                                                </i:InvokeCommandAction>
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                    </TreeViewItem>
                                    <TreeViewItem Header="日志存储" Style="{DynamicResource TreeViewItemStyle2nd}" >
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="PreviewMouseLeftButtonUp">
                                                <i:InvokeCommandAction Command="{Binding ScrollToItem, Mode=OneWay, Source={StaticResource CVMPayout}}" >
                                                    <i:InvokeCommandAction.CommandParameter>
                                                        <MultiBinding Converter="{StaticResource MultiParameter}">
                                                            <Binding ElementName="bdr_Options"/>
                                                            <Binding ElementName="scr_Base"/>
                                                            <Binding ElementName="dck_Base_Log"/>
                                                        </MultiBinding>
                                                    </i:InvokeCommandAction.CommandParameter>
                                                </i:InvokeCommandAction>
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                    </TreeViewItem>
                                </TreeViewItem>
                                <TreeViewItem Header="Radio配置" Style="{DynamicResource TreeViewItemStyleRoot}" >
                                    <i:Interaction.Triggers>
                                        <i:EventTrigger EventName="Selected">
                                            <i:InvokeCommandAction Command="{Binding TreeSelectedChanged, Mode=OneWay, Source={StaticResource CVMPayout}}" CommandParameter="1" />
                                        </i:EventTrigger>
                                    </i:Interaction.Triggers>
                                    <TreeViewItem Header="常规" Style="{DynamicResource TreeViewItemStyle2nd}">
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="PreviewMouseLeftButtonUp">
                                                <i:InvokeCommandAction Command="{Binding ScrollToItem, Mode=OneWay, Source={StaticResource CVMPayout}}" >
                                                    <i:InvokeCommandAction.CommandParameter>
                                                        <MultiBinding Converter="{StaticResource MultiParameter}">
                                                            <Binding ElementName="bdr_Options"/>
                                                            <Binding ElementName="scrview_Radio"/>
                                                            <Binding ElementName="dck_RadioGeneric"/>
                                                        </MultiBinding>
                                                    </i:InvokeCommandAction.CommandParameter>
                                                </i:InvokeCommandAction>
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                    </TreeViewItem>
                                    <TreeViewItem Header="网络" Style="{DynamicResource TreeViewItemStyle2nd}">
                                        <TreeViewItem.Visibility>
                                            <MultiBinding Converter="{StaticResource FalseHide}">
                                                <Binding ElementName="chk_EnableRadio" Path="IsChecked"/>
                                            </MultiBinding>
                                        </TreeViewItem.Visibility>
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="PreviewMouseLeftButtonUp">
                                                <i:InvokeCommandAction Command="{Binding ScrollToItem, Mode=OneWay, Source={StaticResource CVMPayout}}" >
                                                    <i:InvokeCommandAction.CommandParameter>
                                                        <MultiBinding Converter="{StaticResource MultiParameter}">
                                                            <Binding ElementName="bdr_Options"/>
                                                            <Binding ElementName="scrview_Radio"/>
                                                            <Binding ElementName="dck_RadioNetwork"/>
                                                        </MultiBinding>
                                                    </i:InvokeCommandAction.CommandParameter>
                                                </i:InvokeCommandAction>
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                    </TreeViewItem>
                                    <TreeViewItem Header="MNIS" Style="{DynamicResource TreeViewItemStyle2nd}" >
                                        <TreeViewItem.Visibility>
                                            <MultiBinding Converter="{StaticResource FalseHide}">
                                                <Binding ElementName="chk_EnableRadio" Path="IsChecked"/>
                                                <Binding ElementName="rad_RadioMnisType" Path="IsChecked"/>
                                            </MultiBinding>
                                        </TreeViewItem.Visibility>
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="PreviewMouseLeftButtonUp">
                                                <i:InvokeCommandAction Command="{Binding ScrollToItem, Mode=OneWay, Source={StaticResource CVMPayout}}" >
                                                    <i:InvokeCommandAction.CommandParameter>
                                                        <MultiBinding Converter="{StaticResource MultiParameter}">
                                                            <Binding ElementName="bdr_Options"/>
                                                            <Binding ElementName="scrview_Radio"/>
                                                            <Binding ElementName="dck_RadioMnis"/>
                                                        </MultiBinding>
                                                    </i:InvokeCommandAction.CommandParameter>
                                                </i:InvokeCommandAction>
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                    </TreeViewItem>
                                    <TreeViewItem Header="GPS翻转" Style="{DynamicResource TreeViewItemStyle2nd}">
                                        <TreeViewItem.Visibility>
                                            <MultiBinding Converter="{StaticResource FalseHide}">
                                                <Binding ElementName="chk_EnableRadio" Path="IsChecked"/>
                                            </MultiBinding>
                                        </TreeViewItem.Visibility>
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="PreviewMouseLeftButtonUp">
                                                <i:InvokeCommandAction Command="{Binding ScrollToItem, Mode=OneWay, Source={StaticResource CVMPayout}}" >
                                                    <i:InvokeCommandAction.CommandParameter>
                                                        <MultiBinding Converter="{StaticResource MultiParameter}">
                                                            <Binding ElementName="bdr_Options"/>
                                                            <Binding ElementName="scrview_Radio"/>
                                                            <Binding ElementName="dck_RadioGPS"/>
                                                        </MultiBinding>
                                                    </i:InvokeCommandAction.CommandParameter>
                                                </i:InvokeCommandAction>
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                    </TreeViewItem>
                                    <TreeViewItem Header="功能" Style="{DynamicResource TreeViewItemStyle2nd}" Visibility="Collapsed">
                                        <!--<TreeViewItem.Visibility>
                                            <MultiBinding Converter="{StaticResource FalseHide}">
                                                <Binding ElementName="chk_EnableRadio" Path="IsChecked"/>
                                            </MultiBinding>
                                        </TreeViewItem.Visibility>-->
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="PreviewMouseLeftButtonUp">
                                                <i:InvokeCommandAction Command="{Binding ScrollToItem, Mode=OneWay, Source={StaticResource CVMPayout}}" >
                                                    <i:InvokeCommandAction.CommandParameter>
                                                        <MultiBinding Converter="{StaticResource MultiParameter}">
                                                            <Binding ElementName="bdr_Options"/>
                                                            <Binding ElementName="scrview_Radio"/>
                                                            <Binding ElementName="dck_RadioFunc"/>
                                                        </MultiBinding>
                                                    </i:InvokeCommandAction.CommandParameter>
                                                </i:InvokeCommandAction>
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                    </TreeViewItem>
                                </TreeViewItem>
                                <TreeViewItem Header="中继台" Style="{DynamicResource TreeViewItemStyleRoot}">
                                    <i:Interaction.Triggers>
                                        <i:EventTrigger EventName="Selected">
                                            <i:InvokeCommandAction Command="{Binding TreeSelectedChanged, Mode=OneWay, Source={StaticResource CVMPayout}}" CommandParameter="2" />
                                        </i:EventTrigger>
                                    </i:Interaction.Triggers>
                                    <TreeViewItem Header="常规" Style="{DynamicResource TreeViewItemStyle2nd}">
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="PreviewMouseLeftButtonUp">
                                                <i:InvokeCommandAction Command="{Binding ScrollToItem, Mode=OneWay, Source={StaticResource CVMPayout}}" >
                                                    <i:InvokeCommandAction.CommandParameter>
                                                        <MultiBinding Converter="{StaticResource MultiParameter}">
                                                            <Binding ElementName="bdr_Options"/>
                                                            <Binding ElementName="scrview_WireLan"/>
                                                            <Binding ElementName="dck_WireLanGeneric"/>
                                                        </MultiBinding>
                                                    </i:InvokeCommandAction.CommandParameter>
                                                </i:InvokeCommandAction>
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                    </TreeViewItem>
                                    <TreeViewItem Header="网络" Style="{DynamicResource TreeViewItemStyle2nd}">
                                        <TreeViewItem.Visibility>
                                            <MultiBinding Converter="{StaticResource FalseHide}">
                                                <Binding ElementName="chk_EnableWireLan" Path="IsChecked"/>
                                            </MultiBinding>
                                        </TreeViewItem.Visibility>
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="PreviewMouseLeftButtonUp">
                                                <i:InvokeCommandAction Command="{Binding ScrollToItem, Mode=OneWay, Source={StaticResource CVMPayout}}" >
                                                    <i:InvokeCommandAction.CommandParameter>
                                                        <MultiBinding Converter="{StaticResource MultiParameter}">
                                                            <Binding ElementName="bdr_Options"/>
                                                            <Binding ElementName="scrview_WireLan"/>
                                                            <Binding ElementName="dck_WireLanNetwork"/>
                                                        </MultiBinding>
                                                    </i:InvokeCommandAction.CommandParameter>
                                                </i:InvokeCommandAction>
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                    </TreeViewItem>
                                    <TreeViewItem Header="MNIS" Style="{DynamicResource TreeViewItemStyle2nd}">
                                        <TreeViewItem.Visibility>
                                            <MultiBinding Converter="{StaticResource FalseHide}">
                                                <Binding ElementName="chk_EnableWireLan" Path="IsChecked"/>
                                            </MultiBinding>
                                        </TreeViewItem.Visibility>
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="PreviewMouseLeftButtonUp">
                                                <i:InvokeCommandAction Command="{Binding ScrollToItem, Mode=OneWay, Source={StaticResource CVMPayout}}" >
                                                    <i:InvokeCommandAction.CommandParameter>
                                                        <MultiBinding Converter="{StaticResource MultiParameter}">
                                                            <Binding ElementName="bdr_Options"/>
                                                            <Binding ElementName="scrview_WireLan"/>
                                                            <Binding ElementName="dck_WireLanMnis"/>
                                                        </MultiBinding>
                                                    </i:InvokeCommandAction.CommandParameter>
                                                </i:InvokeCommandAction>
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                    </TreeViewItem>
                                    <TreeViewItem Header="Dongle" Style="{DynamicResource TreeViewItemStyle2nd}">
                                        <TreeViewItem.Visibility>
                                            <MultiBinding Converter="{StaticResource FalseHide}">
                                                <Binding ElementName="chk_EnableWireLan" Path="IsChecked"/>
                                            </MultiBinding>
                                        </TreeViewItem.Visibility>
                                        <i:Interaction.Triggers>
                                            <i:EventTrigger EventName="PreviewMouseLeftButtonUp">
                                                <i:InvokeCommandAction Command="{Binding ScrollToItem, Mode=OneWay, Source={StaticResource CVMPayout}}" >
                                                    <i:InvokeCommandAction.CommandParameter>
                                                        <MultiBinding Converter="{StaticResource MultiParameter}">
                                                            <Binding ElementName="bdr_Options"/>
                                                            <Binding ElementName="scrview_WireLan"/>
                                                            <Binding ElementName="dck_WireLanMnis"/>
                                                        </MultiBinding>
                                                    </i:InvokeCommandAction.CommandParameter>
                                                </i:InvokeCommandAction>
                                            </i:EventTrigger>
                                        </i:Interaction.Triggers>
                                    </TreeViewItem>
                                    <TreeViewItem Header="功能" Style="{DynamicResource TreeViewItemStyle2nd}" Visibility="Collapsed"/>
                                </TreeViewItem>
                                <TreeViewItem Header="资源管理" Style="{DynamicResource TreeViewItemStyleRoot}"  Width="195" >
                                    <i:Interaction.Triggers>
                                        <i:EventTrigger EventName="Selected">
                                            <i:InvokeCommandAction Command="{Binding TreeSelectedChanged, Mode=OneWay, Source={StaticResource CVMPayout}}" CommandParameter="3" />
                                        </i:EventTrigger>
                                    </i:Interaction.Triggers>
                                    <TreeViewItem Header="供应商" Style="{DynamicResource TreeViewItemStyle2nd}" Visibility="Collapsed"/>
                                    <TreeViewItem Header="部门" Style="{DynamicResource TreeViewItemStyle2nd}" Visibility="Collapsed"/>
                                    <TreeViewItem Header="组" Style="{DynamicResource TreeViewItemStyle2nd}"/>
                                    <TreeViewItem Header="人员" Style="{DynamicResource TreeViewItemStyle2nd}"/>
                                    <TreeViewItem Header="车辆" Style="{DynamicResource TreeViewItemStyle2nd}" Visibility="Collapsed"/>

                                    <TreeViewItem Header="终端" Style="{DynamicResource TreeViewItemStyle2nd}"/>
                                    <TreeViewItem Header="绑定/分配" Style="{DynamicResource TreeViewItemStyle2nd}"  Visibility="Collapsed"/>
                                </TreeViewItem>
                                <TreeViewItem Header="账号及权限" Style="{DynamicResource TreeViewItemStyleRoot}" >
                                    <i:Interaction.Triggers>
                                        <i:EventTrigger EventName="Selected">
                                            <i:InvokeCommandAction Command="{Binding TreeSelectedChanged, Mode=OneWay, Source={StaticResource CVMPayout}}" CommandParameter="4" />
                                        </i:EventTrigger>
                                    </i:Interaction.Triggers>
                                    <TreeViewItem Header="账户" Style="{DynamicResource TreeViewItemStyle2nd}" />
                                    <TreeViewItem Header="功能分配" Style="{DynamicResource TreeViewItemStyle2nd}" />
                                </TreeViewItem>
                                <TreeViewItem Header="室内定位" Style="{DynamicResource TreeViewItemStyleRoot}" >
                                    <i:Interaction.Triggers>
                                        <i:EventTrigger EventName="Selected">
                                            <i:InvokeCommandAction Command="{Binding TreeSelectedChanged, Mode=OneWay, Source={StaticResource CVMPayout}}" CommandParameter="5" />
                                        </i:EventTrigger>
                                    </i:Interaction.Triggers>
                                    <TreeViewItem Header="常规" Style="{DynamicResource TreeViewItemStyle2nd}" />
                                    <TreeViewItem Header="网络" Style="{DynamicResource TreeViewItemStyle2nd}" />
                                    <TreeViewItem Header="区域" Style="{DynamicResource TreeViewItemStyle2nd}" />
                                    <TreeViewItem Header="蓝牙信标（iBeacon）" Style="{DynamicResource TreeViewItemStyle2nd}" />
                                    <TreeViewItem Header="信标位置" Style="{DynamicResource TreeViewItemStyle2nd}" />
                                </TreeViewItem>
                                <TreeViewItem Header="地图" Style="{DynamicResource TreeViewItemStyleRoot}" Visibility="Collapsed" >
                                    <i:Interaction.Triggers>
                                        <i:EventTrigger EventName="Selected">
                                            <i:InvokeCommandAction Command="{Binding TreeSelectedChanged, Mode=OneWay, Source={StaticResource CVMPayout}}" CommandParameter="6" />
                                        </i:EventTrigger>
                                    </i:Interaction.Triggers>
                                    <TreeViewItem Header="常规" Style="{DynamicResource TreeViewItemStyle2nd}" />
                                    <TreeViewItem Header="BMP地图" Style="{DynamicResource TreeViewItemStyle2nd}" />
                                </TreeViewItem>
                                <TreeViewItem Header="注册认证" Style="{DynamicResource TreeViewItemStyleRoot}">
                                    <i:Interaction.Triggers>
                                        <i:EventTrigger EventName="Selected">
                                            <i:InvokeCommandAction Command="{Binding TreeSelectedChanged, Mode=OneWay, Source={StaticResource CVMPayout}}" CommandParameter="7" />
                                        </i:EventTrigger>
                                    </i:Interaction.Triggers>
                                    <TreeViewItem Header="软件注册" Style="{DynamicResource TreeViewItemStyle2nd}" />
                                </TreeViewItem>
                            </TreeView>
                        </Grid>
                    </Border>
                    <Border BorderBrush="#FF0C0C0D" Background="#FF313133" Margin="0" BorderThickness="1">
                        <DockPanel>
                            <Label Content="{Binding SelectedItem.Header, ElementName=tab_Options}" Background="#FF3B3B3D" VerticalContentAlignment="Center" Margin="0" Height="31" Foreground="#FFE4E4E4" BorderThickness="0,0,0,1" BorderBrush="#FF0C0C0D" FontSize="14" FontFamily="Hiragino Sans GB W3" Padding="20,5,5,5" DockPanel.Dock="Top"/>
                            <Border x:Name="bdr_Options" BorderBrush="Black" ClipToBounds="True" DockPanel.Dock="Top">
                                <TabControl x:Name="tab_Options" Margin="0,-22,0,0" BorderThickness="0" Background="{x:Null}" SelectedIndex="{Binding TabIndex, Mode=OneWay, Source={StaticResource CVMPayout}}">
                                    <TabItem Header="基本设置">
                                        <ScrollViewer x:Name="scr_Base" VerticalScrollBarVisibility="Auto" Padding="20,2.5" Margin="0,0,18,18.76" Style="{DynamicResource ScrollViewerStyleNormal}">
                                            <DockPanel LastChildFill="False" Margin="0">
                                                <DockPanel x:Name="dck_Base_Net"  LastChildFill="False" DockPanel.Dock="Top" VerticalAlignment="Top" Margin="0,0,0,30">
                                                    <DockPanel Height="24" LastChildFill="False" DockPanel.Dock="Top" Margin="0,20,0,0">
                                                        <Image Source="images/double_arrow_13_10.png" Stretch="UniformToFill" HorizontalAlignment="Left" VerticalAlignment="Center" Width="13" Height="10"/>
